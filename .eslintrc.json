{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
          }
    },
    "plugins": [
        "prettier"
      ],
      "rules": {
        "prettier/prettier": "error"
      },
      "allowTemplateLiterals": false,
      "avoidEscape": true
}